{
  "name": "markdown-github-dark",
  "version": "0.1.0",
  "displayName": "Dark Github Markdown Preview",
  "description": "Replaces VS Code's markdown preview with Github's dark theme style",
  "keywords": [
    "markdown",
    "markdown style",
    "github",
    "github markdown",
    "style"
  ],
  "icon": "logo.png",
  "publisher": "ozaki",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/OzakIOne/markdown-github-dark.git"
  },
  "bugs": {
    "url": "https://github.com/OzakIOne/markdown-github-dark/issues"
  },
  "engines": {
    "vscode": "^1.51.0"
  },
  "categories": [
    "Other"
  ],
  "contributes": {
    "markdown.previewStyles": [
      "./base.css",
      "./github-markdown.css",
      "./node_modules/highlight.js/styles/github-dark.css"
    ]
  },
  "dependencies": {
    "highlight.js": "^11.2.0"
  },
  "devDependencies": {
    "github-markdown-css": "^4.0.0"
  },
  "scripts": {
    "build": "node ./build/update-github-markdown-css.js"
  }
}
